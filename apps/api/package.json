{
  "name": "api",
  "version": "0.1.0",
  "author": {
    "name": "Vlad Cuciureanu",
    "email": "vlad@cuciureanu.net",
    "url": "https://vlad.cuciureanu.net"
  },
  "private": true,
  "scripts": {
    "prebuild": "yarn run prisma:generate || npm run prisma:generate",
    "build": "tsc",
    "start": "node -r esbuild-register ./src/index.ts",
    "clean": "rm -rf dist",
    "dev": "tsx watch ./src/index.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:db:push": "export $(grep -v '^#' .env | xargs -d '\n') && prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^5.21.0",
    "@saeki/schema": "*",
    "argon2": "^0.41.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@saeki/eslint-config": "*",
    "@saeki/typescript-config": "*",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^9.13.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.0",
    "tsx": "^4.19.1",
    "typescript": "^5.3.3"
  }
}
